<div>Formulario para registro de user</div>
  <div>
    <form action="/auth/signup" method="POST">
      <label for="name">Name:</label>
      <input
        type="text"
        id="name"
        name="name"
        placeholder="Name"
        value="{{name}}"
        required
      />
      <button>Save</button>
    </form>
  </div>